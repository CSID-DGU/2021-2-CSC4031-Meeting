{
  "manifest_version": 2,

  "name": "Please_note",
  "version": "1.1.1",
  "icons": {
    "128":"icon.png"
    },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Open Chrome Audio Capture interface"
  },
  "background": {
    "scripts": ["background.js", "worker.js"],
    "persistent": true
  },
  "permissions": [
    "tabCapture",
    "downloads",
    "storage"
  ],
  "commands": {
  }
}
